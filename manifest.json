{
  "manifest_version": 2,
  "name": "Clarin y La Naci√≥n Sin LoginWall",
  "description": "Quita el molesto LoginWall de: Clarin.com, Lanacion.com.ar y Ole.com.ar",
  "version": "1.8",
  "icons": { "16": "16x16.png",
             "48": "48x48.png",
             "128": "128x128.png" },
  "permissions": [
    "tabs", 
    "webRequest",
    "webRequestBlocking",
